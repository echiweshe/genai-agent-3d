# Environment variables for GenAI Agent 3D LLM Services
# Add your API keys below

# OpenAI API key
OPENAI_API_KEY=

# Anthropic API key
ANTHROPIC_API_KEY=

# Hunyuan3D API key (if applicable)
HUNYUAN3D_API_KEY=

# Redis connection (default is localhost)
REDIS_URL=redis://localhost:6379

# Default LLM provider and model
DEFAULT_LLM_PROVIDER=Ollama
DEFAULT_LLM_MODEL=llama3:latest

# LLM Service concurrency settings
MAX_CONCURRENT_REQUESTS=5
MAX_QUEUE_SIZE=100
REQUEST_TIMEOUT=120

# Worker settings
WORKER_HEARTBEAT_INTERVAL=60
